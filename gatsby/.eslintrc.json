{
	"extends": ["wesbos"],
	"rules": {
		"semi": ["error", "never"],
		"no-console": "off",
		"prettier/prettier": [
			"error",
			{
				"trailingComma": "es5",
				"singleQuote": true,
				"printWidth": 120,
				"tabWidth": 2,
				"semi": false
			}
		]
	},
	"parser": "babel-eslint",
	"parserOptions": {
		"sourceType": "module",
		"allowImportExportEverywhere": true
	}
}
